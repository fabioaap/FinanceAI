{
  "name": "@financeai/sync-engine",
  "version": "0.1.0",
  "description": "Sync engine for Finance AI - synchronizes local data with cloud storage",
  "type": "module",
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/index.js",
      "types": "./dist/index.d.ts"
    }
  },
  "scripts": {
    "build": "tsc",
    "lint": "eslint src --ext .ts",
    "lint:fix": "eslint src --ext .ts --fix",
    "test": "vitest run",
    "test:watch": "vitest",
    "type-check": "tsc --noEmit",
    "prepublishOnly": "npm run build"
  },
  "keywords": [
    "sync",
    "offline-first",
    "dexie",
    "supabase",
    "firebase"
  ],
  "author": "",
  "license": "MIT",
  "dependencies": {
    "dexie": "^4.0.10"
  },
  "devDependencies": {
    "@types/node": "^20.10.0",
    "@typescript-eslint/eslint-plugin": "^6.13.0",
    "@typescript-eslint/parser": "^6.13.0",
    "eslint": "^8.54.0",
    "fake-indexeddb": "^6.2.5",
    "jsdom": "^27.2.0",
    "typescript": "^5.3.2",
    "vitest": "^1.0.4"
  },
  "peerDependencies": {
    "@supabase/supabase-js": "^2.38.0"
  },
  "peerDependenciesMeta": {
    "@supabase/supabase-js": {
      "optional": true
    }
  }
}
